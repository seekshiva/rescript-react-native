{
  "name": "rescript-react-native-for-rescript-11",
  "description": "ReScript bindings for React Native.",
  "version": "0.70.0",
  "peerDependencies": {
    "@rescript/react": ">=0.12.0",
    "react-native": ">=0.70.0",
    "rescript": ">=11.0.0"
  },
  "overrides": {
    "react": "18.0.0"
  },
  "repository": "https://github.com/seekshiva/rescript-react-native.git",
  "funding": "https://github.com/seekshiva/rescript-react-native?sponsor=1",
  "license": "MIT",
  "keywords": [
    "rescript",
    "react-native"
  ],
  "files": [
    "*.md",
    "bsconfig.json",
    "src/**/*.res",
    "src/**/*.resi",
    "src/**/*.js",
    "!src/**/*.bs.js"
  ],
  "scripts": {
    "format:most": "prettier --write \"**/*.{md,json,js,css}\"",
    "format:res": "rescript format -all",
    "format": "npm run format:most && npm run format:res",
    "re:start": "rescript build -w",
    "re:build": "rescript",
    "re:clean-build": "rescript clean && rescript",
    "start": "npm run re:start",
    "build": "npm run re:build",
    "test": "npm run re:clean-build",
    "release": "npmpub"
  },
  "devDependencies": {
    "@rescript/react": "^0.12.1",
    "husky": "^4.0.0",
    "lint-staged": "^10.0.0",
    "npm-run-all": "^3.0.0",
    "npmpub": "^5.0.0",
    "prettier": "^2.0.0",
    "rescript": "^11.0.1"
  },
  "prettier": {
    "trailingComma": "all"
  },
  "lint-staged": {
    "*.{md,json,js,css}": "prettier --write",
    "*.{res,resi}": "rescript format"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "packageManager": "yarn@1.22.19+sha1.4ba7fc5c6e704fce2066ecbfb0b0d8976fe62447"
}
